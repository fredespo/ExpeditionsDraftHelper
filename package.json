  "bugs": {
    "url": "https://github.com/fredespo/ExpeditionsDraftHelper/issues"
  },
  "homepage": "https://github.com/fredespo/ExpeditionsDraftHelper#readme",
  "devDependencies": {
    "bindings": "~1.2.1",
    "electron": "^15.5.5",
    "electron-packager": "^14.1.0",
    "electron-rebuild": "^1.8.6",
    "frameless-titlebar": "^1.0.8",
    "node-addon-api": "^1.0.0",
    "node-gyp": "^6.0.1"
  },
  "dependencies": {
    "electron-log": "^3.0.9",
    "xmlhttprequest": "^1.8.0"
